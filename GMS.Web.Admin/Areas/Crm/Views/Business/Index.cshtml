            font-size: 13px;
        }

        .serchtype {
            font-size: 10px;
            display: inline-block;
            margin: 0px;
        }

            .serchtype * {
                word-break: keep-all;
                padding: 0px;
            }

        #lasthead, #lastfoot {
            min-width: 260px;
        }

        th {
            word-break: keep-all;
            word-wrap: inherit;
        }

        td table tbody {
            display: initial;
        }

        td table, td table tr {
            width: 100% !important;
            display: inline-block;
        }

        .controls label {
            display: inline;
        }

        .custom-combobox input {
            height: 28px;
            margin: 1px;
        }

        .custom-combobox a {
            height: 30px;
        }

        .bsns {
            font-size: 8px;
            padding: 5px;
            width: 100%;
            display: inline-block;
            word-break: break-all;
        }

            .bsns span {
                background-color: lightblue;
                word-break: keep-all;
                width: 100%;
                height: 100%;
                display: inline-block;
            }

        .bigdrop {
            width: 230px !important;
        }
